            class="fr.legrain.majcomptelegrainfr.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:outils">
         <command
               commandId="fr.legrain.majCompteLegrainFr.editors.EditorMajCompteLegrainFr"
               icon="icons/export_wiz.gif"
               label="Mise à jour des comptes legrain.fr"
               style="push">
         </command>
         <separator
               name="fr.legrain.majCompteLegrainFr.separator1"
               visible="true">
         </separator>
      </menuContribution>
   </extension>
